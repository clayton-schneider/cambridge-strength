---
// Import components
import { Icon } from "astro-icon";
// import NavMenu from "./NavMenu";
const links = [
  { linkText: "Home", link: "/" },
  { linkText: "About", link: "/about" },
  { linkText: "Clients", link: "/clients" },
  { linkText: "Services", link: "/services" },
  { linkText: "Contact", link: "/contact" },
];

interface Props {
  className?: string;
}
const { className } = Astro.props;
---

<header class={(className ? className : "") + " px-edge py-10 relative"}>
  <div
    class="max-w-screen-2xl mx-auto py-4 flex justify-between gap-5 xl:block"
  >
    <div class="flex items-center justify-between">
      <a href="/">
        <Icon
          name="logo"
          class="mx-auto lg:mx-0 fill-white w-[200px]"
          alt="Cambridge Strength logo"
        />
      </a>

      <div class="hidden xl:block space-x-7"></div>
    </div>

    <!-- <NavMenu links={links} client:load /> -->
  </div>
</header>
